<!-- Version 0.0.1 -->
<nodeDefs>

  <!-- Node server node - KEEP THIS. This will create a nodeserver node showing if the node server is active or not -->
  <nodedef id="CONTROLLER" nls="CONTROLLER">
    <sts>
      <st id="ST" editor="CONTROLLER"/>
    </sts>
    <cmds>
      <sends/>
      <accepts>
        <cmd id="DISCOVER"/>
        <cmd id="CREATE_NEW"/>
        <cmd id="UPDATE_PROFILE"/>
        <cmd id="REMOVE_NOTICES"/>
        <cmd id="QUERY"/>
      </accepts>
    </cmds>
  </nodedef>

  <nodeDef id="SONOS_SPEAKER" nls="SPEAKER">
    <editors />
    <sts>
        <st id="ST" editor="PSTATUS" />
        <st id="SVOL" editor="SVOL" />
        <st id="GV0" editor="bool" />  <!-- Mute/unMute -->
        <st id="GV1" editor="bool" />  <!-- Shuffle -->
        <st id="GV2" editor="bool" />  <!-- Bass -->
        <st id="GV3" editor="bool" />  <!-- Treble -->
        <st id="GV4" editor="bool" />  <!-- Coordinator -->
        <st id="GV5" editor="_56_0" />  <!-- Members -->

    </sts>
    <cmds>
        <sends />
        <accepts>
            <cmd id="SVOL">
                <p id="" editor="SVOL" init="SVOL" />
            </cmd>
            <cmd id="PLAYLST">
                <p id="" editor="PLIST" init="PLAYLST" />
            </cmd>
            <cmd id="FAV">
                <p id="" editor="PFAV" init="FAV" />
            </cmd>
            <cmd id="SAYTTS">
                <p id="" editor="SAYTTS" init="SAYTTS" />
            </cmd>
            <cmd id="PLAY" />
            <cmd id="PAUSE" />
            <cmd id="PREVIOUS" />
            <cmd id="NEXT" />
            <cmd id="MUTE" />
            <cmd id="UNMUTE" />
            <cmd id="SHUFFLEON" />
            <cmd id="SHUFFLEOFF" />
        </accepts>
    </cmds>
    </nodeDef>

</nodeDefs>
